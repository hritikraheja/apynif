  "source": "//SPDX-License-Identifier: Unlicensed\r\npragma solidity ^0.8.4;\r\n\r\nimport './NFT.sol';\r\n\r\n/**\r\nThis contract defines various structs that are used in the other contracts (NFT.sol, Collections.sol, \r\nBusinesses.sol and Marketplace.sol).\r\n */\r\n\r\ncontract Definitions{\r\n    struct NftDetails{\r\n        uint256 nftId;\r\n        uint256 price;\r\n        address seller;\r\n        bool isSold;\r\n        bool isListed;\r\n        string category;\r\n    }\r\n\r\n    struct Nft{\r\n        NftDetails details;\r\n        string uri;\r\n    }\r\n\r\n    struct Collection{\r\n        uint256 collectionId;\r\n        string collectionName;\r\n        string collectionCoverImageUri;\r\n        string collectionDescription;\r\n        string collectionOwnerName;\r\n        address payable collectionOwnerAddress;\r\n        uint256 nftCount;\r\n        uint256[] nftIds;\r\n        bool isListed;\r\n        string category;\r\n    }\r\n\r\n    struct Business{\r\n        uint256 businessId;\r\n        string businessName;\r\n        string businessLogoUri;\r\n        uint256 feePercent;\r\n        address adminAddress;\r\n        uint256 collectionCount;\r\n        uint256 singleNftCount;\r\n        uint256[] nftIds;\r\n        uint256[] collectionIds;\r\n        address[] employees;\r\n    }\r\n\r\n    struct NftOnAuction{\r\n        uint256 nftId;\r\n        address highestBidder;\r\n        uint256 highestBid;\r\n        uint256 bid_end_time;\r\n    }\r\n}",
